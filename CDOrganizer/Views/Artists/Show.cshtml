<h3>Here are all the CDs for this artist:</h3>

<ol>
@foreach (var CD in @Model["CDs"])
{
  <li><a href='/artists/@Model["artist"].GetId()/artists/@artist.GetId()'>@CD.GetTitle()</a></li>
}
</ol>
<h1>CD List CD Details: </h1>

<h2>@Model["CD"].GetTitle()</h2>
<h3>Artist: @Model["artist"].GetName()</h3>

<a href='/artists/@Model["artist"].GetId()/artists/new'>Add another CD</a>
<a href='/artists'>View all artists</a>
